          "a[href*='/checkout']",
          "input[name='checkout']"
        ]
      }
    },
    "generic_ecommerce": {
      "domains": [],
      "urlPatterns": ["/cart", "/checkout", "/product", "/shop"],
      "selectors": {
        "productLinks": [
          "a[href*='/product']",
          "a.product-link",

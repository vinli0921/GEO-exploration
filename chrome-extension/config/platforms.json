        "checkout": [
          "button[data-automation-id='checkout']",
          "a[href*='/checkout']"
        ]
      }
    },
    "target": {
      "domains": ["target.com", "www.target.com"],
      "selectors": {
        "searchInput": [
          "input[type='search']",
          "input[data-test='@web/Search/SearchInput']"
        ],
        "productLinks": [
          "a[data-test='product-title']",
          "a[href*='/p/']"
        ],
        "addToCart": [

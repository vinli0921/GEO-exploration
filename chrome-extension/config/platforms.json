          ".response-container",
          "message-content",
          ".markdown-container"
        ],
        "resultLinks": [
          ".model-response-text a[href^='http']",
          ".response-container a[href^='http']"
        ]
      }
    },
    "bing_copilot": {
      "domains": ["copilot.microsoft.com"],
      "urlPatterns": [],
      "selectors": {
        "queryInput": [
          "#userInput",
          "textarea[data-testid='composer-input']",
          "textarea[placeholder*='Copilot']"
        ],
        "responseContainer": [
          "[data-testid='composer-content']",
          ".cib-serp-main",
          "[data-content='Copilot']"
        ],
        "aiResultLinks": [
          "a[href^='http']"
        ]
      }
    },
    "bing_ai": {
      "domains": ["bing.com", "www.bing.com"],
      "urlPatterns": [],
      "selectors": {
        "queryInput": [
          "#sb_form_q",
          "textarea[placeholder*='Ask']",
          ".b_searchbox"

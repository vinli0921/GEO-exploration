        ],
        "checkout": [
          "button[data-test='checkout-button']",
          "a[href*='/checkout']"
        ]
      }
    },
    "ebay": {
      "domains": ["ebay.com", "www.ebay.com"],
      "selectors": {
        "searchInput": [
          "input[type='text'][name='_nkw']",
          "#gh-ac"
        ],
        "productLinks": [
          ".s-item__link",
          "a[href*='/itm/']"
        ],
        "addToCart": [

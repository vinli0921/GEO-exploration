        "checkout": [
          "button[data-test-id='cta-btn']"
        ]
      }
    },
    "shopify": {
      "domains": [],
      "urlPatterns": ["/products/", "/cart", "/checkouts/"],
      "selectors": {
        "productLinks": [
          "a[href*='/products/']",

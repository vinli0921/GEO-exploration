  "ecommerce_platforms": {
    "amazon": {
      "domains": ["amazon.com", "www.amazon.com", "amazon.co.uk", "amazon.ca"],
      "selectors": {
        "searchInput": [
          "#twotabsearchtextbox",
          "input[type='text'][name='field-keywords']"
        ],
        "productLinks": [
          "a.s-product-image-container",
          "h2 a.a-link-normal",
          "[data-component-type='s-search-result'] a.a-link-normal",
          "a[href*='/dp/']"
        ],
        "addToCart": [
          "#add-to-cart-button",
          "input[name='submit.add-to-cart']",
          "#add-to-cart-button-ubb"
        ],
        "buyNow": [
          "#buy-now-button",
          "input[name='submit.buy-now']"
        ],
        "checkout": [
          "input[name='proceedToRetailCheckout']",
          "a[href*='spc/checkout']",
          "button[name='proceedToCheckout']"
        ]
      }
    },
    "walmart": {
      "domains": ["walmart.com", "www.walmart.com"],
      "selectors": {
        "searchInput": [
          "input[type='search']",
          "input[name='q']"
        ],
        "productLinks": [
          "a[link-identifier='itemTitle']",

{
  "ai_platforms": {
    "chatgpt": {
      "domains": ["chat.openai.com", "chatgpt.com"],
      "selectors": {
        "queryInput": [
          "div#prompt-textarea[contenteditable='true']",
          "div[contenteditable='true'].ProseMirror",
          "textarea#prompt-textarea:not([style*='display: none'])",
          "textarea[placeholder*='Message']"
        ],
        "submitButton": [
          "button[data-testid='send-button']",
          "button[data-testid='fruitjuice-send-button']"
        ],
        "responseContainer": [
          ".markdown",
          ".text-base",
          "[data-message-author-role='assistant']",
          ".agent-turn"
        ],
        "resultLinks": [
          ".markdown a[href^='http']",
          "[data-message-author-role='assistant'] a[href^='http']"
        ]
      }
    },
    "perplexity": {
      "domains": ["perplexity.ai", "www.perplexity.ai"],
      "selectors": {
        "queryInput": [
          "#ask-input",
          "div[contenteditable='true'][data-lexical-editor='true']",
          "div[contenteditable='true'][role='textbox']",
          "textarea[placeholder*='Ask']",
          "textarea[placeholder*='follow']",
          "input[type='search']",
          "textarea.search-input"
        ],
        "submitButton": [
          "button[data-testid='submit-button']",
          "button[aria-label='Submit']",
          "button[type='submit']"
        ],
        "responseContainer": [
          ".answer-container",
          "[class*='Answer']",
          ".prose",
          "[class*='SearchResult']"
        ],
        "citations": [
          ".citation",
          "[data-citation]",
          ".source",
          "[class*='Citation']"
        ],
        "resultLinks": [
          ".citation a",
          ".source-link",
          "[class*='Citation'] a"
        ]
      }
    },
    "google_ai": {
      "domains": ["google.com", "www.google.com"],
      "urlPatterns": ["/search"],
      "selectors": {
        "queryInput": [
          "textarea[name='q']",
          "input[name='q']",
          "textarea[aria-label*='Search']"
        ],
        "aiOverview": [
          "[data-al*='AI overview']",
          ".YzCcne",
          "[jsname='dEwkXc']",
          ".hdzaWe",
          "[data-attrid='SGPQuestionAnswer']",
          ".AI-Overview",
          "[jsname='yp1CPe']",
          ".xpdopen",
          "[data-hveid*='AI']"
        ],
        "organicResults": [
          "#search .g",
          ".g",
          "[data-sokoban-container]"
        ],
        "aiResultLinks": [
          ".AI-Overview a[href^='http']",
          "[data-attrid='SGPQuestionAnswer'] a[href^='http']"
        ]
      }
    },
    "claude": {
      "domains": ["claude.ai"],
      "selectors": {
        "queryInput": [
          "textarea[data-testid='chat-input-ssr']",
          "textarea[aria-label*='prompt to Claude']",
          "textarea[placeholder*='help you today']",
          "div[contenteditable='true']",
          "div[contenteditable='true'][data-placeholder]",
          ".ProseMirror"
        ],
        "submitButton": [
          "button[aria-label='Send Message']",
          "button[type='submit']"
        ],
        "responseContainer": [
          "[data-is-streaming]",
          ".font-claude-message",
          "[data-test-render-count]",
          ".whitespace-pre-wrap"
        ],
        "resultLinks": [
          ".font-claude-message a[href^='http']",
          "[data-is-streaming] a[href^='http']"
        ]
      }
    },
    "gemini": {
      "domains": ["gemini.google.com", "bard.google.com"],
      "urlPatterns": ["/app/"],
      "selectors": {
        "queryInput": [
          ".ql-editor[contenteditable='true'][aria-label*='prompt']",
          ".ql-editor[contenteditable='true']",
          "div.ql-editor",
          "rich-textarea .ql-editor",
          "div[contenteditable='true'][aria-label*='prompt']"
        ],
        "submitButton": [
          "button[aria-label='Send']",
          "button[mattooltip='Send']"
        ],
        "responseContainer": [

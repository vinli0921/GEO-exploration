        "aiResponse": [
          ".gs_h.gs_caphead",
          ".gs_temp_content",
          ".gs_caphead_main",
          ".gs_heroTextHeader",
          ".b_ans",
          ".cib-serp-main"
        ],
        "aiResultLinks": [
          ".b_ans a[href^='http']",
          ".cib-serp-main a[href^='http']"
        ]
      }
    }
  },
  "search_platforms": {
    "google_search": {
